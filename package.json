{
  "name": "fake-pev-shopping",
  "version": "1.0.0",
  "description": "Fake Personal Electric Vehicle Shopping application built with React. Browse, compare, buy, review vehicles and their accessories.",
  "author": "ScriptyChris",
  "license": "ISC",
  "homepage": "https://github.com/ScriptyChris/Fake-PEV-Shopping#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ScriptyChris/Fake-PEV-Shopping.git"
  },
  "keywords": [
    "shopping",
    "pev",
    "react"
  ],
  "bugs": {
    "url": "https://github.com/ScriptyChris/Fake-PEV-Shopping/issues"
  },
  "scripts": {
    "test": "npm run eslint:test && jest --env=node",
    "test:dev": "jest --watch --env=node",
    "test:debug": "node --inspect-brk node_modules/jest/bin/jest.js --runInBand",
    "prettier": "prettier --write src/ !src/frontend/assets !src/database/data",
    "prettier:backend": "prettier --write src/middleware/ src/database/ !src/database/data",
    "eslint:backend": "set \"BACKEND_ONLY=true\" && eslint src/middleware/** src/database/**",
    "eslint:test": "set \"BACKEND_ONLY=true\" && set \"TEST_ENV=true\" && eslint \"test/**\" \"__mocks__/*\" \"src/**/__mocks__/*\"",
    "serve": "set \"BACKEND_ONLY=true\" && node dist/src/middleware/middleware-index.js",
    "frontend": "set \"BACKEND_ONLY=false\" && webpack-dev-server",
    "backend": "set \"BACKEND_ONLY=true\" && nodemon --exec \"npm run eslint:backend && npm run prettier:backend && node src/middleware/middleware-index.js\"",
    "prebuild": "npm run prettier",
    "build:backend": "rimraf dist/src dist/utils && tsc --project tsconfig.backend.json",
    "build:frontend": "webpack",
    "build": "npm run build:backend && npm run build:frontend"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.3",
    "@types/mongoose-paginate-v2": "^1.3.9",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "css-loader": "^4.2.1",
    "eslint": "^7.7.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.20.6",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^0.10.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "prettier-loader": "^3.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "sass-loader": "^9.0.3",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "jsonwebtoken": "^8.5.1",
    "mobx": "^5.15.6",
    "mobx-react": "^6.3.0",
    "mongodb": "3.5.10",
    "mongoose": "5.9.27",
    "mongoose-paginate-v2": "^1.3.13",
    "react-paginate": "^7.0.0",
    "react-simple-tree-menu": "^1.1.18"
  }
}
